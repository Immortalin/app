<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "KP3JVQ46W2.com.mdstand.app"
        versionCode = "12" 
        version     = "1.0.5" >

  <!-- versionCode is optional and Android only -->

  <name>MDStand</name>
  <description>
    MDStand tablet app.
  </description>

  <content src="index.html" />

  <gap:platform name="ios" />
  
  <icon src="icon.png" />
  <gap:splash src="splash.png" />
  <!-- <gap:splash src="Default-Landscape.png" gap:platform="ios" width="1024" height="768" /> -->

  <access origin="https://mdstand.com" subdomains="true" />
  <access origin="https://mdstand-resources.s3.amazonaws.com" />

  <preference name="phonegap-version" value="3.6.3" />
  <preference name="fullscreen" value="true" />
  <preference name="target-device" value="tablet" />
  <preference name="exit-on-suspend" value="false" />
  <preference name="orientation" value="landscape" />
  <preference name="prerendered-icon" value="true" />

  <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" version="1.2.0" />
  <gap:plugin name="hu.dpal.phonegap.plugins.uniquedeviceid" version="1.2.0" />
  <gap:plugin name="com.badrit.macaddress" version="0.1.0" />
  
  <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
  <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
  <gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
  <gap:plugin name="com.jcesarmobile.idfvplugin" version="1.0.0" />

</widget>
